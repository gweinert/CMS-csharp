@model CMS.Models.PageElement

<div>
    <h1>Create New Image</h1>
    <form asp-action="CreateImage" enctype="multipart/form-data">
        <label asp-for="Name"></label>
        <input type="text" asp-for="Name" />
        <input type="file" name="TempFile" id="TempFile" multiple>
        <input type="hidden" asp-for="Type" value="Type.Image">
        <input asp-for="PageID" type="hidden" value="@(ViewData["PageID"])">
        <button type="submit">Create</button>
    </form>
</div>